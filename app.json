{
  "expo": {
    "name": "Initial_app",
    "slug": "Initial_app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ivansidney.Initial"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
        
      },
      
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ivansidney.Initial"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "jsEngine": "hermes",
    "extra": {
      "eas": {
        "projectId": "cd4fbad8-92a7-4fc6-9458-6d1f2f8907ae"
      }
    }
  }
}
